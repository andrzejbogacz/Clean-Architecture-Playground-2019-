<?xml version="1.0" encoding="utf-8"?>

<ScrollView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="#F1F1F1"


        android:id="@+id/asdas"
        >

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical"
            android:paddingStart="10dp"
            android:paddingTop="20dp"
            android:paddingEnd="10dp"
            android:paddingBottom="10dp"
            tools:context=".view.profile.ProfileActivity"

            >

        <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/iv_photo3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/shape_profile_details"
                android:onClick="swapView"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toEndOf="@+id/iv_photo2"
                android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toStartOf="@+id/iv_photo4"
                app:layout_constraintBottom_toBottomOf="@+id/iv_photo2"
                app:layout_constraintTop_toTopOf="@+id/iv_photo2"
                />


        <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/iv_photo2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/shape_profile_details"
                android:adjustViewBounds="true"
                app:layout_constraintDimensionRatio="1:1"

                app:layout_constraintStart_toStartOf="@+id/guideline6"
                android:layout_marginTop="8dp"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toStartOf="@+id/iv_photo3"
                app:layout_constraintTop_toBottomOf="@+id/iv_photo1"
                />

        <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/iv_photo1"
                android:layout_width="0dp"
                android:layout_height="0dp"

                android:background="@drawable/shape_profile_details"
                app:layout_constraintDimensionRatio="1:1"
                android:onClick="swapView"
                app:layout_constraintStart_toStartOf="@+id/guideline6"
                app:layout_constraintEnd_toEndOf="@+id/iv_photo3"
                android:layout_marginTop="32dp"
                app:layout_constraintTop_toTopOf="parent"
                />

        <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/iv_photo4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"

                android:background="@drawable/shape_profile_details"
                android:onClick="swapView"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/iv_photo3"
                android:layout_marginStart="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_photo3"
                app:layout_constraintEnd_toEndOf="@+id/iv_photo5"
                app:layout_constraintTop_toTopOf="@+id/iv_photo3"
                />


        <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/iv_photo5"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"
                android:background="@drawable/shape_profile_details"
                android:onClick="swapView"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/iv_photo4"
                app:layout_constraintEnd_toStartOf="@+id/guideline7"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/iv_photo6"
                app:layout_constraintEnd_toEndOf="@+id/iv_photo6"
                />

        <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/iv_photo6"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/shape_profile_details"
                android:onClick="swapView"
                app:layout_constraintBottom_toTopOf="@+id/iv_photo5"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintTop_toTopOf="@+id/iv_photo1"
                app:layout_constraintStart_toStartOf="@+id/iv_photo5"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline7"
                />

        <include
                android:id="@+id/include"
                layout="@layout/view_profile_user_details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"

                android:layout_marginTop="32dp"
                app:layout_constraintTop_toBottomOf="@+id/iv_photo3"
                />

        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/guideline6"
                app:layout_constraintGuide_percent="0.09245742"
                />

        <androidx.constraintlayout.widget.Guideline
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/guideline7"
                app:layout_constraintGuide_percent="0.91240877"
                />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>

