<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		>

	<data>

	</data>

	<ScrollView
			android:id="@+id/layout_profile_scrollView"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#F1F1F1"
			>

		<androidx.constraintlayout.widget.ConstraintLayout
				android:id="@+id/layout"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:background="#F1F1F1"
				android:orientation="vertical"
				android:paddingStart="10dp"
				android:paddingTop="20dp"
				android:paddingEnd="10dp"
				tools:context=".view.profile.ProfileFragment"
				>

			<com.makeramen.roundedimageview.RoundedImageView
					android:id="@+id/iv_photo3"
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_marginStart="8dp"
					android:background="#FFFFFF"
					android:elevation="5dp"
					android:onClick="swapView"
					app:layout_constraintBottom_toBottomOf="@+id/iv_photo2"
					app:layout_constraintDimensionRatio="1:1"
					app:layout_constraintEnd_toStartOf="@+id/iv_photo4"
					app:layout_constraintHorizontal_bias="0.5"
					app:layout_constraintStart_toEndOf="@+id/iv_photo2"
					app:layout_constraintTop_toTopOf="@+id/iv_photo2"
					/>


			<com.makeramen.roundedimageview.RoundedImageView
					android:id="@+id/iv_photo2"
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_marginTop="8dp"
					android:adjustViewBounds="true"
					android:background="#FFFFFF"
					android:elevation="5dp"
					app:layout_constraintDimensionRatio="1:1"
					app:layout_constraintEnd_toStartOf="@+id/iv_photo3"
					app:layout_constraintHorizontal_bias="0.5"
					app:layout_constraintStart_toStartOf="@+id/guideline6"
					app:layout_constraintTop_toBottomOf="@+id/iv_photo1"
					/>

			<com.makeramen.roundedimageview.RoundedImageView
					android:id="@+id/iv_photo1"
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_marginTop="32dp"
					android:background="#FFFFFF"
					android:elevation="5dp"
					android:onClick="swapView"
					android:scaleType="fitCenter"
					app:layout_constraintDimensionRatio="1:1"
					app:layout_constraintEnd_toEndOf="@+id/iv_photo3"
					app:layout_constraintStart_toStartOf="@+id/guideline6"
					app:layout_constraintTop_toTopOf="parent"
					/>

			<com.makeramen.roundedimageview.RoundedImageView
					android:id="@+id/iv_photo4"
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_marginStart="8dp"
					android:background="#FFFFFF"
					android:elevation="5dp"
					android:onClick="swapView"
					app:layout_constraintBottom_toBottomOf="@+id/iv_photo3"
					app:layout_constraintDimensionRatio="1:1"
					app:layout_constraintEnd_toEndOf="@+id/iv_photo5"
					app:layout_constraintHorizontal_bias="0.5"
					app:layout_constraintStart_toEndOf="@+id/iv_photo3"
					app:layout_constraintTop_toTopOf="@+id/iv_photo3"
					/>

			<com.makeramen.roundedimageview.RoundedImageView
					android:id="@+id/iv_photo5"
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_marginEnd="8dp"
					android:layout_marginBottom="8dp"
					android:background="#FFFFFF"
					android:elevation="5dp"
					android:onClick="swapView"
					app:layout_constraintBottom_toTopOf="@+id/iv_photo4"
					app:layout_constraintDimensionRatio="1:1"
					app:layout_constraintEnd_toEndOf="@+id/iv_photo6"
					app:layout_constraintEnd_toStartOf="@+id/guideline7"
					app:layout_constraintTop_toBottomOf="@+id/iv_photo6"
					/>

			<com.makeramen.roundedimageview.RoundedImageView
					android:id="@+id/iv_photo6"
					android:layout_width="0dp"
					android:layout_height="0dp"
					android:layout_marginEnd="8dp"
					android:layout_marginBottom="8dp"
					android:background="#FFFFFF"
					android:elevation="5dp"
					android:onClick="swapView"
					app:layout_constraintBottom_toTopOf="@+id/iv_photo5"
					app:layout_constraintDimensionRatio="1:1"
					app:layout_constraintEnd_toStartOf="@+id/guideline7"
					app:layout_constraintStart_toStartOf="@+id/iv_photo5"
					app:layout_constraintTop_toTopOf="@+id/iv_photo1"
					/>

			<include
					android:id="@+id/include"
					layout="@layout/view_profile_user_details"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="32dp"
					android:layout_marginBottom="16dp"
					app:layout_constraintBottom_toBottomOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toBottomOf="@+id/iv_photo3"
					app:layout_constraintVertical_bias="0.0"
					/>

			<androidx.constraintlayout.widget.Guideline
					android:id="@+id/guideline6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					app:layout_constraintGuide_percent="0.09245742"
					/>

			<androidx.constraintlayout.widget.Guideline
					android:id="@+id/guideline7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					app:layout_constraintGuide_percent="0.91240877"
					/>

			<androidx.constraintlayout.widget.Guideline
					android:id="@+id/guideline4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:orientation="horizontal"
					app:layout_constraintGuide_begin="379dp"
					/>

		</androidx.constraintlayout.widget.ConstraintLayout>
	</ScrollView>
</layout>

